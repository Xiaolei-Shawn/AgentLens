{
  "name": "agentlens-monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "install:all": "pnpm install",
    "build": "pnpm -r build",
    "lint": "pnpm -r --if-present lint",
    "dev:webapp": "pnpm --filter webapp dev",
    "start:mcp": "pnpm --filter @xiaolei.shawn/mcp-server mcp",
    "start:gateway": "pnpm --filter @xiaolei.shawn/mcp-server start",
    "ingest": "pnpm --filter @xiaolei.shawn/mcp-server ingest",
    "ingest:auto": "pnpm --filter @xiaolei.shawn/mcp-server ingest:auto",
    "ingest:codex": "pnpm --filter @xiaolei.shawn/mcp-server ingest:codex",
    "ingest:cursor": "pnpm --filter @xiaolei.shawn/mcp-server ingest:cursor"
  }
}
